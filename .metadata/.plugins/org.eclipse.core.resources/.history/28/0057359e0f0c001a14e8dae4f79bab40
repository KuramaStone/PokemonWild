public class test {

	public static void main(String[] args) {

		int[] images = { 1, 2, 3, 4, 5, 6, 7, 8, 9, 0 };
		int[] dest = new int[images.length - 1];

		long start = System.nanoTime();
		System.arraycopy(images, 0, dest, 0, images.length - 1);
		System.out.println(System.nanoTime() - start);
		

		dest = new int[images.length - 1];
		start = System.nanoTime();
		for(int i = 0; i < dest.length; i++) {
			dest[i] = images[i];
		}
		System.out.println(System.nanoTime() - start);
	}

}
