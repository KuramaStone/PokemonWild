package me.brook.PokemonCreator.world.tile;

public class TileData {
	
	private TileType type;
	private int variant;
	
	public TileData(TileType type, int variant) {
		this.type = type;
		this.variant = variant;
	}
	
	public TileType getType() {
		return type;
	}
	public int getVariant() {
		return variant;
	}

}
